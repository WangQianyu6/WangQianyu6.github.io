ine"><span class="type">int</span> dfn[maxn], low[maxn];</span><br><span class="line"><span class="type">int</span> bcc_cnt = <span class="number">0</span>;        <span class="comment">// è¾¹åŒè¿é€šåˆ†é‡æ•°é‡</span></span><br><span class="line"><span class="built_in">vector</span>&lt;<span class="type">int</span>&gt; bcc[maxn];  <span class="comment">// è®°å½•æ¯ä¸ªç‚¹åŒè¿é€šåˆ†é‡é‡Œé¢çš„ç‚¹ å› ä¸ºå°±ç»è¿‡ä¸€æ¬¡å°±è£…åœ¨vectoré‡Œ</span></span><br><span class="line"><span class="built_in">stack</span>&lt;<span class="type">int</span>&gt; S;</span><br><span class="line"><span class="type">void</span> <span class="title function_">dfs</span><span class="params">(<span class="type">int</span> u, <span class="type">int</span> fa)</span> &#123;</span><br><span class="line">    dfn[u] = low[u] = ++times;</span><br><span class="line">    S.push(u);</span><br><span class="line">    <span class="keyword">for</span> (<span class="type">int</span> i = p[u]; i != <span class="number">-1</span>; i = E[i].next) &#123;</span><br><span class="line">        <span class="type">int</span> v = E[i].v;</span><br><span class="line">        <span class="keyword">if</span> (dfn[v] == <span class="number">0</span>) &#123;  <span class="comment">// v æ²¡æœ‰è¢«è®¿é—®è¿‡ï¼Œu, v æ˜¯æ ‘è¾¹</span></span><br><span class="line">            dfs(v, u);</span><br><span class="line">            low[u] = min(low[u], low[v]);<span class="comment">// æ ¹æ®lowæ•°ç»„çš„å«ä¹‰</span></span><br><span class="line">            </span><br><span class="line">            <span class="comment">/*if(low[v]&gt;dfn[u]) æ¡¥++ï¼›*/</span></span><br><span class="line">            </span><br><span class="line">        &#125; <span class="keyword">else</span> <span class="keyword">if</span> (dfn[v] &lt; dfn[u] &amp;&amp; v != fa) &#123;  <span class="comment">// åå‘è¾¹ï¼Œæ³¨æ„ v == fa çš„æ—¶å€™ï¼Œæ˜¯è®¿é—®é‡å¤çš„è¾¹</span></span><br><span class="line">            low[u] = min(low[u], dfn[v]);</span><br><span class="line">        &#125;</span><br><span class="line">    &#125;</span><br><span class="line">    <span class="keyword">if</span> (low[u] == dfn[u]) &#123;  <span class="comment">// æ­¤æ—¶ u æ˜¯æ ¹ç»“ç‚¹æˆ–è€… fa -&gt; u æ˜¯æ¡¥</span></span><br><span class="line">        ++bcc_cnt;           <span class="comment">// å¢åŠ ä¸€ä¸ªè¾¹åŒè¿é€šåˆ†é‡</span></span><br><span class="line">        <span class="keyword">while</span> (!S.empty()) &#123;  <span class="comment">//ä»æ ˆä¸­å¼¹å‡º u åŠ u ä¹‹åçš„é¡¶ç‚¹</span></span><br><span class="line">            <span class="type">int</span> x = S.top();</span><br><span class="line">            S.pop();</span><br><span class="line">            bcc[bcc_cnt].push_back(x);</span><br><span class="line">            <span class="keyword">if</span> (x == u) <span class="keyword">break</span>;  </span><br><span class="line">        &#125;</span><br><span class="line">    &#125;</span><br><span class="line">&#125;</span><br><span class="line"><span class="type">int</span> <span class="title function_">main</span><span class="params">()</span> &#123;</span><br><span class="line">    init();</span><br><span class="line">    <span class="type">int</span> n, m;</span><br><span class="line">    <span class="built_in">cin</span> &gt;&gt; n &gt;&gt; m;</span><br><span class="line">    <span class="keyword">for</span> (<span class="type">int</span> i = <span class="number">0</span>; i &lt; m; ++i) &#123;</span><br><span class="line">        <span class="type">int</span> u, v;</span><br><span class="line">        <span class="built_in">cin</span> &gt;&gt; u &gt;&gt; v;</span><br><span class="line">        insert(u, v);</span><br><span class="line">        insert(v, u);</span><br><span class="line">    &#125;</span><br><span class="line">    <span class="built_in">memset</span>(dfn, <span class="number">0</span>, <span class="keyword">sizeof</span>(dfn));</span><br><span class="line">    times=bcc_cnt=<span class="number">0</span>;</span><br><span class="line">  